<?xml version="0.0" ?>
<robot name="model.urdf">
  <link name="baseLink">
    <contact>
      <friction_anchor/>
      <lateral_friction value="0.3"/>
      <rolling_friction value="0.0"/>
      <contact_cfm value="0.0"/>
      <contact_erp value="1.0"/>
    </contact>

    <!-- Inertial properties -->
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 -0.03"/> <!-- Moved center to origin -->
      <mass value="0.1"/>
      <inertia ixx="8e-05" ixy="0" ixz="0" iyy="0.0002" iyz="0" izz="0.0002"/>
    </inertial>

    <!-- Visual -->
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.03"/> <!-- Moved visual to center -->
      <geometry>
        <mesh filename="textured.obj" scale="1 1 1"/> 
      </geometry>
    </visual>

    <!-- Collision -->
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.03"/> <!-- Moved collision to center -->
      <geometry>
        <mesh filename="collision.obj" scale="1 1 1"/>
      </geometry>
    </collision>

  </link>
</robot>